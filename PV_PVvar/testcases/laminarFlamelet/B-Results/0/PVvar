FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      PVvar;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0; 

boundaryField
{
    open
    {
        type            zeroGradient;
    }
    closed
    {
        type            zeroGradient;
    }
    sides
    {
        type            empty;
    }
}


