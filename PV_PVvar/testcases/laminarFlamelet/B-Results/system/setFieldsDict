FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
//    volScalarFieldValue T    INITIALTEMPERATURE
    volScalarFieldValue PV   0.0
);

regions
(
    boxToCell
    {
        box (1.5e-2 -1.0 -1.0) (5.0e-2 1.0 1.0) ;
        fieldValues
        (
//            volScalarFieldValue T   FINALTEMPERATURE
            volScalarFieldValue PV  8.5714364356152e-02
        );
    }
);

